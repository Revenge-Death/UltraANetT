<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="DBEngine"
                   namespace="DBEngine">

  <class name="VehicelAuthorization">
    <id name="VehicelType" />
    <id name="VehicelConfig" />
    <id name="VehicelStage" />
    <property name="CreateTime" />
    <property name="Creater" />
    <property name="AuthorizeTo" />
    <property name="AuthorizedFromDept" />
    <property name="AuthorizationTime" />
    <property name="InvaildTime" />
    <property name="Remark" />
  </class>
  
  <class name="Empolyee">
    <id name="No" />
    <id name="Name" />
    <id name="Role" />
    <property name="Department" />
    <property name="Sex" />
    <property name="TelePhone" />
    <property name="Mail" />
    <property name="Password" />
    <!--<property name="Portrait" />-->
    <property name="Remark" />
  </class>
  
  <class name="Department">
    <id name="No" />
    <id name="Name" />
    <id name="Master" />
    <property name="NumberInDept" />
    <property name="Remark" />
  </class>
</hibernate-mapping>